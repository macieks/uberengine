<?xml version="1.0" encoding="utf-8"?>
<asset parent="" type="font">
  <paramGroup name="all">
    <param name="compiler" value="FontCompiler_FreeType.exe"/>
    <param name="source"/>
    <param name="characterSetSource" value="string"/>
    <param name="characterString" value="0123456789abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ!@#$%^*()_+-=[]{}|:;',./?&lt;&gt;&amp;&quot; "/>
    <param name="characterFilePath" value=""/>
    <param name="sizeInPixels" value="32"/>
    <param name="numTextureLevels" value="1"/>
    <param name="bold" value="false"/>
    <param name="italic" value="false"/>
    <param name="underlined" value="false"/>
	<param name="packIntoSeparateChannels" value="true"/>
	<param name="textureFormat" value="B8G8R8A8"/>
  </paramGroup>
  <paramGroup name="marmalade">
	<param name="packIntoSeparateChannels" value="false"/>
	<param name="textureFormat" value="R8G8B8A8"/>
  </paramGroup>
  <paramGroup name="marmalade-ios">
	<param name="packIntoSeparateChannels" value="false"/>
	<param name="makePow2Texture" value="true"/>
	<param name="textureFormat" value="R8G8B8A8"/>
  </paramGroup>
  <paramGroup name="linux">
    <param name="compiler" value="FontCompiler_FreeType.run"/>
  </paramGroup>
  <paramGroup name="win32-gles">
    <param name="textureFormat" value="R8G8B8A8"/>
  </paramGroup>
</asset>
