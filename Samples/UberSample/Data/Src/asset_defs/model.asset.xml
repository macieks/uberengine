  </paramGroup>
  <paramGroup name="win32-bullet-d3d9">
    <param name="compiler" value="ModelCompiler_Assimp.exe"/>
    <param name="physicsCompiler" value="PhysicsCompiler_Bullet.dll"/>
  </paramGroup>
  <paramGroup name="x360-physx">
    <param name="compiler" value="ModelCompiler_Assimp_X360.exe"/>
    <param name="physicsCompiler" value="PhysicsCompiler_PhysX_X360.dll"/>
  </paramGroup>
  <paramGroup name="ps3-physx">
